{
	"manifest_version": 2,
	"name": "Abuse Saver",
	"version": "1.1",
	"author": "Léo Carvalho",
	"description": "Identifica se o site está abusando do processamento do dispositivo e bateria.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons":
		{
			"16": "icons/16-icon.png",
			"32": "icons/32-icon.png",
			"48": "icons/48-icon.png",
			"64": "icons/64-icon.png",
			"96": "icons/96-icon.png",
			"128": "icons/128-icon.png"
		}
}

